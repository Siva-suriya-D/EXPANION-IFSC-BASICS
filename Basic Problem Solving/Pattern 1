#include<stdio.h>
int main(int argc,char* argv[]){
    int n;
    int i,j,temp,t;
    scanf("%d",&n);
    temp=n;
    t=n-(n-1);
    for(i=n;i>1;i--)
    {
        for(j=1;j<i;j++)
        {
            printf("%d ",j);
        }
        for(j=temp;j==i;j--)
        {
            printf("%d",i);
            temp--;
        }
        printf("\n");
    }
    printf("%d",t);
return 0;
}
